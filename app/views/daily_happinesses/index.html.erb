<h1>Listing daily_happinesses</h1>

<table>
  <thead>
    <tr>
      <th>Time</th>
      <th>Happiness score</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @daily_happinesses.each do |daily_happiness| %>
      <tr>
        <td><%= daily_happiness.time %></td>
        <td><%= daily_happiness.happiness_score %></td>
        <td><%= link_to 'Show', daily_happiness %></td>
        <td><%= link_to 'Edit', edit_daily_happiness_path(daily_happiness) %></td>
        <td><%= link_to 'Destroy', daily_happiness, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Daily happiness', new_daily_happiness_path %>
